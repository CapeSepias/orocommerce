# Configuration for https://github.com/oroinc/phpstan-rules/
trusted_data:
    variables:
        Oro\Bundle\RedirectBundle\Async\SluggableEntitiesProcessor:
            getEntityIds:
                identifierFieldName: true
        Oro\Bundle\RedirectBundle\Api\Processor\ComputeUrlFields:
            loadUrls:
                ownerEntityClass: true
                ownerEntityIdFieldName: true

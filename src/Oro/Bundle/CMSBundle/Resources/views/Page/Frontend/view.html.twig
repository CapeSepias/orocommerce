{% extends 'OroFrontendBundle:actions:view.html.twig' %}

{% oro_title_set({params : {"%title%": entity.title}}) %}

{% block pageHeader %}
    {% set breadcrumbs = {
        'entity': entity,
        'entityTitle': entity.title
    } %}
    {{ parent() }}
{% endblock pageHeader %}

{% block stats %}{% endblock stats %}
{% block navButtons %}{% endblock navButtons %}

{% block ownerLink %}{% endblock ownerLink %}

{% block content_data %}
    <div class="frontend-page-container">
        {{ entity.content|oro_html_sanitize_wysiwyg }}
    </div>
{% endblock content_data %}
